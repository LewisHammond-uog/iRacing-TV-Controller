<Window x:Class="iRacingTVController.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:iRacingTVController"
        mc:Ignorable="d"
        Title="iRacing-TV Controller v1.31"
        WindowStartupLocation="CenterScreen"
        Closed="Window_Closed" ResizeMode="CanResizeWithGrip" Width="900" Height="700" MinWidth="900" MinHeight="700" Icon="/Assets/icon/iracing-tv.ico">
    <Grid>
        <TabControl>
            <TabItem Header="Control Panel" Padding="12,6,12,6">
                <Grid Margin="6,6,6,6">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="90"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Content="Race Status" FontSize="10" FontWeight="Bold" />
                        <Separator Margin="6,0,0,0" Grid.Column="1"/>
                    </Grid>
                    <Grid Margin="0,6,0,0" Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel Width="90" Margin="0,0,3,0">
                            <TextBox x:Name="ControlPanel_RaceStatus_SimFrame" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontFamily="Courier New" FontSize="11"/>
                            <Label Content="Sim Frame" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                        <StackPanel Width="120" Margin="3,0,0,0" Grid.Column="1">
                            <TextBox x:Name="ControlPanel_RaceStatus_SessionName" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                            <Label Content="Session Name" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                        <StackPanel Width="120" Margin="3,0,0,0" Grid.Column="2">
                            <TextBox x:Name="ControlPanel_RaceStatus_SessionType" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                            <Label Content="Session Type" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                        <StackPanel Width="120" Margin="3,0,0,0" Grid.Column="3">
                            <TextBox x:Name="ControlPanel_RaceStatus_SessionState" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                            <Label Content="Session State" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                        <StackPanel Width="60" Margin="3,0,3,0" Grid.Column="4">
                            <TextBox x:Name="ControlPanel_RaceStatus_SimTime" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                            <Label Content="Sim Time" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                        <StackPanel Width="60" Margin="3,0,3,0" Grid.Column="5">
                            <TextBox x:Name="ControlPanel_RaceStatus_RemainingTime" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontFamily="Courier New" FontSize="11"/>
                            <Label Content="Rem. Time" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                        <StackPanel Width="60" Margin="3,0,3,0" Grid.Column="6">
                            <TextBox x:Name="ControlPanel_RaceStatus_RemainingLaps" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontFamily="Courier New" FontSize="11"/>
                            <Label Content="Rem. Laps" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                        </StackPanel>
                    </Grid>
                    <Grid Margin="0,6,0,0" Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="90"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Content="Camera Control" FontSize="10" FontWeight="Bold" />
                        <Separator Margin="6,0,0,0" Grid.Column="1"/>
                    </Grid>
                    <Grid Margin="0,6,0,0" Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid Margin="0,0,3,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <StackPanel Width="60" Margin="0,0,3,0">
                                    <TextBox x:Name="ControlPanel_CameraControl_Timer" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontFamily="Courier New" FontSize="11"/>
                                    <Label Content="Timer" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                                </StackPanel>
                                <StackPanel Width="60" Margin="3,0,3,0" Grid.Column="1">
                                    <TextBox x:Name="ControlPanel_CameraControl_CarNumber" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                                    <Label Content="Car Number" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                                </StackPanel>
                                <StackPanel Width="150" Margin="3,0,3,0" Grid.Column="2">
                                    <TextBox x:Name="ControlPanel_CameraControl_DriverName" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                                    <Label Content="Driver Name" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                                </StackPanel>
                                <StackPanel Width="90" Margin="3,0,3,0" Grid.Column="3">
                                    <TextBox x:Name="ControlPanel_CameraControl_CameraGroup" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                                    <Label Content="Camera Group" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                                </StackPanel>
                                <StackPanel Width="360" Margin="3,0,0,0" Grid.Column="4">
                                    <TextBox x:Name="ControlPanel_CameraControl_Reason" Padding="3,0,3,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" Height="17" FontSize="11"/>
                                    <Label Content="Reason" FontSize="9" HorizontalAlignment="Center" Margin="0,-3,0,0" />
                                </StackPanel>
                            </Grid>
                            <Grid x:Name="ControlPanel_ButtonGrid" Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                            </Grid>
                        </Grid>
                        <StackPanel Width="90" Margin="3,0,0,0" Grid.Column="1">
                            <Button x:Name="ControlPanel_CameraControl_Enable_Button" Height="25" Padding="3,0,3,0" Content="🎥 Enable" Click="ControlPanel_CameraControl_Enable_Button_Click"/>
                            <Separator Margin="0,6,0,6"/>
                            <Button x:Name="ControlPanel_CameraControl_Director_Button" Height="25" Padding="3,0,3,0" Content="Director" Click="ControlPanel_CameraControl_Director_Button_Click"/>
                            <Button x:Name="ControlPanel_CameraControl_Manual_Button" Height="25" Padding="3,0,3,0" Content="Manual" Click="ControlPanel_CameraControl_Manual_Button_Click"/>
                            <Separator Margin="0,6,0,6"/>
                            <Button x:Name="ControlPanel_Camera_Scenic_Button" Content="Scenic" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_Scenic_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_Pits_Button" Content="Pits" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_Pits_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_StartFinish_Button" Content="S/F" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_StartFinish_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_Inside_Button" Content="Inside" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_Inside_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_Close_Button" Content="Close" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_Close_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_Medium_Button" Content="Medium" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_Medium_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_Far_Button" Content="Far" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_Far_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_VeryFar_Button" Content="Very Far" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_VeryFar_Button_Click"/>
                            <Button x:Name="ControlPanel_Camera_AutoCam_Button" Content="AutoCam" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_AutoCam_Button_Click"/>
                            <Separator Margin="0,6,0,6"/>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="ControlPanel_Camera_C1_Button" Content="C1" Width="45" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_C1_Button_Click"/>
                                <Button x:Name="ControlPanel_Camera_C2_Button" Content="C2" Width="45" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_C2_Button_Click"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="ControlPanel_Camera_C3_Button" Content="C3" Width="45" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_C3_Button_Click"/>
                                <Button x:Name="ControlPanel_Camera_C4_Button" Content="C4" Width="45" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_C4_Button_Click"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="ControlPanel_Camera_C5_Button" Content="C5" Width="45" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_C5_Button_Click"/>
                                <Button x:Name="ControlPanel_Camera_C6_Button" Content="C6" Width="45" Height="25" Padding="3,0,3,0" Click="ControlPanel_Camera_C6_Button_Click"/>
                            </StackPanel>
                        </StackPanel>
                    </Grid>
                    <Grid Margin="0,6,0,0" Grid.Row="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="90"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Content="Overlay Control" FontSize="10" FontWeight="Bold" />
                        <Separator Margin="6,0,0,0" Grid.Column="1"/>
                    </Grid>
                    <Grid Margin="0,6,0,0" Grid.Row="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="35"/>
                            <ColumnDefinition Width="35"/>
                            <ColumnDefinition Width="35"/>
                            <ColumnDefinition Width="35"/>
                            <ColumnDefinition Width="35"/>
                            <ColumnDefinition Width="35"/>
                        </Grid.ColumnDefinitions>
                        <ToggleButton x:Name="ControlPanel_Master_Button" Content="MASTER" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" />
                        <ToggleButton x:Name="ControlPanel_RaceStatus_Button" Content="Race Status" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="1" />
                        <ToggleButton x:Name="ControlPanel_Leaderboard_Button" Content="Leaderboard" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="2" />
                        <ToggleButton x:Name="ControlPanel_TrackMap_Button" Content="Track Map" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="3" />
                        <ToggleButton x:Name="ControlPanel_StartLights_Button" Content="Start Lights" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="4" />
                        <ToggleButton x:Name="ControlPanel_VoiceOf_Button" Content="Voice Of" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="5" />
                        <ToggleButton x:Name="ControlPanel_Subtitles_Button" Content="Subtitles" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="6" />
                        <ToggleButton x:Name="ControlPanel_Intro_Button" Content="Intro" Height="25" Padding="3,0,3,0" Margin="0,0,6,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="7" />
                        <ToggleButton x:Name="ControlPanel_C1_Button" Content="C1" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="8" />
                        <ToggleButton x:Name="ControlPanel_C2_Button" Content="C2" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="9" />
                        <ToggleButton x:Name="ControlPanel_C3_Button" Content="C3" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="10" />
                        <ToggleButton x:Name="ControlPanel_C4_Button" Content="C4" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="11" />
                        <ToggleButton x:Name="ControlPanel_C5_Button" Content="C5" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="12" />
                        <ToggleButton x:Name="ControlPanel_C6_Button" Content="C6" Height="25" Padding="3,0,3,0" Checked="ControlPanel_ToggleButton_Updated" Unchecked="ControlPanel_ToggleButton_Updated" Grid.Column="13" />
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Session Flags" Padding="12,6,12,6">
                <Grid Margin="6,6,6,6">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <DockPanel>
                        <ListView x:Name="SessionFlags_ListView">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="#" Width="50">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding Index}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="SN" Width="50">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding SessionNumber}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Session Time" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding SessionTime}" />
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Session Flags" Width="520">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding SessionFlagsAsString}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Actions" Width="50">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button Content="Edit" Click="SessionFlags_Edit_Button_Click" Padding="6,0,6,0"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                            <ListView.Resources>
                                <Style TargetType="ListViewItem">
                                    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                </Style>
                            </ListView.Resources>
                        </ListView>
                    </DockPanel>
                    <Grid Grid.Row="1" Margin="0,6,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Button Content="Add New Session Flags at Current Frame" Click="SessionFlags_AddAtCurrentFrame_Button_Click"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Incidents" Padding="12,6,12,6">
                <Grid Margin="6,6,6,6">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <DockPanel>
                        <ListView x:Name="Incidents_ListView" MouseDoubleClick="Incidents_ListView_MouseDoubleClick">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="#" Width="50">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding Index}" IsHitTestVisible="False"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Frame" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding FrameNumber}" IsHitTestVisible="False"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Car Number" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding CarNumber}" IsHitTestVisible="False"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Driver Name" Width="240">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding DriverName}" IsHitTestVisible="False"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Start" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <local:Number Value="{Binding StartFrame}" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" ValueChanged="Incidents_StartFrame_ValueChanged"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="End" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <local:Number Value="{Binding EndFrame}" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" ValueChanged="Incidents_EndFrame_ValueChanged"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Ignore" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox IsChecked="{Binding Ignore}" Click="Incidents_Ignore_Click"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                            <ListView.Resources>
                                <Style TargetType="ListViewItem">
                                    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                </Style>
                                <Style TargetType="CheckBox" BasedOn="{StaticResource {x:Type CheckBox}}">
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </ListView.Resources>
                        </ListView>
                    </DockPanel>
                    <Grid Grid.Row="1" Margin="0,6,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Button Content="Scan Now" Margin="0,0,3,0" Click="Incidents_ScanNow_Button_Click"/>
                        <Button Content="Clear All" Grid.Column="1" Margin="3,0,0,0" Click="Incidents_Clear_Button_Click"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Subtitles" Padding="12,6,12,6">
                <Grid Margin="6,6,6,6">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <DockPanel>
                        <ListView x:Name="Subtitles_ListView" MouseDoubleClick="Subtitles_ListView_MouseDoubleClick">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn Header="#" Width="50">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding Index}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="SN" Width="50">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding SessionNumber}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Start" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding StartFrame}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="End" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Label Content="{Binding EndFrame}"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Text" Width="390">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <TextBox Text="{Binding Text}" TextChanged="Subtitles_Text_TextChanged"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                    <GridViewColumn Header="Ignore" Width="100">
                                        <GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox IsChecked="{Binding Ignore}" Click="Subtitles_Ignore_Click"/>
                                            </DataTemplate>
                                        </GridViewColumn.CellTemplate>
                                    </GridViewColumn>
                                </GridView>
                            </ListView.View>
                            <ListView.Resources>
                                <Style TargetType="ListViewItem">
                                    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
                                </Style>
                                <Style TargetType="CheckBox" BasedOn="{StaticResource {x:Type CheckBox}}">
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                </Style>
                            </ListView.Resources>
                        </ListView>
                    </DockPanel>
                    <Grid Grid.Row="1" Margin="0,6,0,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Button Content="Clear All" Click="Subtitles_Clear_Button_Click"/>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Director" Padding="12,6,12,6">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid Margin="6,6,6,6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Label Content="Director File" />
                        <ComboBox x:Name="DirectorList" Grid.Column="1" VerticalContentAlignment="Center" SelectionChanged="Director_DirectorList_SelectionChanged" />
                        <Button x:Name="DirectorFile_Rename" Content="Rename" Grid.Column="2" Margin="2,0,0,0" Padding="6,0,6,0" Click="Director_DirectorFile_Rename_Click" />
                        <Button x:Name="DirectorFile_Create" Content="Create" Grid.Column="3" Margin="2,0,0,0" Padding="6,0,6,0" Click="Director_DirectorFile_Create_Click" />
                        <Button x:Name="DirectorFile_Delete" Content="Delete" Grid.Column="4" Margin="2,0,0,0" Padding="6,0,6,0" Click="Director_DirectorFile_Delete_Click" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <TabControl TabStripPlacement="Left">
                            <TabItem Header="Cameras" Padding="12,6,12,6">
                                <ScrollViewer>
                                    <StackPanel Margin="6,6,6,6">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="100"/>
                                                <ColumnDefinition Width="*"/>
