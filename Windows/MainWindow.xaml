                                    </Grid>
                                    <Grid Margin="0,6,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Label Content="Hold Time"/>
                                        <local:Decimal x:Name="Overlay_Intro_HoldTime" ValueChanged="Overlay_Intro_Update" Grid.Column="1" />
                                        <CheckBox x:Name="Overlay_Intro_HoldTime_Override" VerticalAlignment="Center" Content="Override" Click="Overlay_Intro_Update" Grid.Column="2" HorizontalAlignment="Right" Margin="6,0,0,0" />
                                    </Grid>
                                    <Grid Margin="0,6,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Label Content="Out Time"/>
                                        <local:Decimal x:Name="Overlay_Intro_OutTime" ValueChanged="Overlay_Intro_Update" Grid.Column="1" />
                                        <CheckBox x:Name="Overlay_Intro_OutTime_Override" VerticalAlignment="Center" Content="Override" Click="Overlay_Intro_Update" Grid.Column="2" HorizontalAlignment="Right" Margin="6,0,0,0" />
                                    </Grid>
                                </StackPanel>
                            </TabItem>
                        </TabControl>
                    </Grid>
                </Grid>
            </TabItem>
            <TabItem Header="Web Page" Padding="12,6,12,6">
                <TabControl TabStripPlacement="Left">
                    <TabItem Header="General" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="Enable" Width="100"/>
                                <CheckBox x:Name="WebPage_General_Enabled" VerticalAlignment="Center" Click="WebPage_Update" />
                            </StackPanel>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Source Folder" Width="100"/>
                                <Grid Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <TextBox x:Name="WebPage_General_SourceFolder" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="WebPage_Update"/>
                                    <Button x:Name="WebPage_General_SourceFolder_Button" Padding="6,0,6,0" Content="📁" Grid.Column="1" Margin="-1,0,0,0" Click="WebPage_General_SourceFolder_Button_Click" />
                                </Grid>
                            </Grid>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Output Folder" Width="100"/>
                                <Grid Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>
                                    <TextBox x:Name="WebPage_General_OutputFolder" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="WebPage_Update"/>
                                    <Button x:Name="WebPage_General_OutputFolder_Button" Padding="6,0,6,0" Content="📁" Grid.Column="1" Margin="-1,0,0,0" Click="WebPage_General_OutputFolder_Button_Click" />
                                </Grid>
                            </Grid>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="150"/>
                                </Grid.ColumnDefinitions>
                                <Button Padding="6,0,6,0" Content="Reload Template" Grid.Column="1" Height="25" Click="WebPage_General_ReloadTemplate"/>
                            </Grid>
                        </StackPanel>
                    </TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="iRacing" Padding="12,6,12,6">
                <TabControl TabStripPlacement="Left">
                    <TabItem Header="General" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="Command Rate" Width="100"/>
                                <local:Decimal x:Name="iRacing_General_CommandRateLimit" Width="100" ValueChanged="iRacing_Update" />
                                <Label Content="Commands per second"/>
                            </StackPanel>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Account" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Username" Width="100"/>
                                <TextBox x:Name="iRacing_Account_Username" Grid.Column="1" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="iRacing_Update"/>
                            </Grid>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Password" Width="100"/>
                                <PasswordBox x:Name="iRacing_Account_Password" Grid.Column="1" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" PasswordChanged="iRacing_Update"/>
                            </Grid>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="100"/>
                                </Grid.ColumnDefinitions>
                                <Button Padding="6,0,6,0" Content="Connect" Grid.Column="1" Height="25" Click="iRacing_DataApi_Connect"/>
                            </Grid>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Custom Paints" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Label Content="Directory" Width="100"/>
                                <TextBox x:Name="iRacing_CustomPaints_Directory" Grid.Column="1" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="iRacing_Update"/>
                                <Button Padding="6,0,6,0" Content="📁" Grid.Column="2" Margin="-1,0,0,0" Click="iRacing_CustomPaintsDirectory_Button_Click" />
                            </Grid>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Driver Names" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Suffixes" Width="100"/>
                                <TextBox x:Name="iRacing_DriverNames_Suffixes" Grid.Column="1" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="iRacing_Update"/>
                            </Grid>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Format" Width="100"/>
                                <ComboBox x:Name="iRacing_DriverNames_FormatOption" Grid.Column="1" VerticalContentAlignment="Center" SelectionChanged="iRacing_Update" />
                            </Grid>
                            <Grid Margin="0,6,0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Label Content="Capitalization" Width="100"/>
                                <ComboBox x:Name="iRacing_DriverNames_CapitalizationOption" Grid.Column="1" VerticalContentAlignment="Center" SelectionChanged="iRacing_Update" />
                            </Grid>
                        </StackPanel>
                    </TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="Settings" Padding="12,6,12,6">
                <TabControl TabStripPlacement="Left">
                    <TabItem Header="Window" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="Topmost" Width="100"/>
                                <CheckBox x:Name="Editor_AlwaysOnTop" VerticalAlignment="Center" Content="Always On Top" Click="Editor_Update" />
                            </StackPanel>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Mouse" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="Positioning" Width="100"/>
                                <Label Content="Normal" />
                                <local:Decimal x:Name="Editor_Mouse_PositioningSpeedNormal" Width="100" ValueChanged="Editor_Update" />
                                <Label Content="Fast (Shift)" HorizontalContentAlignment="Center" Margin="6,0,0,0" />
                                <local:Decimal x:Name="Editor_Mouse_PositioningSpeedFast" Width="100" ValueChanged="Editor_Update" />
                                <Label Content="Slow (Ctrl)" HorizontalContentAlignment="Center" Margin="6,0,0,0" />
                                <local:Decimal x:Name="Editor_Mouse_PositioningSpeedSlow" Width="100" ValueChanged="Editor_Update" />
                            </StackPanel>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="Incidents" Padding="12,6,12,6">
                        <StackPanel Margin="6,6,6,6">
                            <StackPanel Orientation="Horizontal">
                                <Label Content="Scenic Cameras" Width="100"/>
                                <TextBox x:Name="Editor_Incidents_ScenicCameras" Width="590" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="Editor_Update"/>
                                <Button Padding="6,0,6,0" Content="🎥" Margin="-1,0,0,0" Click="Editor_Incident_ScenicCameras_Button_Click" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="0,6,0,0">
                                <Label Content="Edit Cameras" Width="100"/>
                                <TextBox x:Name="Editor_Incidents_EditCameras" Width="590" Height="25" Padding="3,0,3,0" VerticalContentAlignment="Center" TextChanged="Editor_Update"/>
                                <Button Padding="6,0,6,0" Content="🎥" Margin="-1,0,0,0" Click="Editor_Incident_EditCameras_Button_Click" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="0,6,0,0">
                                <Label Content="Overlap Merge" Width="100"/>
                                <local:Decimal x:Name="Editor_Incidents_OverlapMergeTime" Width="100" ValueChanged="Editor_Update" />
                                <Label Content="Seconds"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Margin="0,6,0,0">
                                <Label Content="Timeout" Width="100"/>
                                <local:Decimal x:Name="Editor_Incidents_Timeout" Width="100" ValueChanged="Editor_Update" />
                                <Label Content="Seconds"/>
                            </StackPanel>
                        </StackPanel>
                    </TabItem>
                </TabControl>
            </TabItem>
        </TabControl>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,6,6,0">
            <Image x:Name="ConnectionStatusImage" Source="/Assets/status-disconnected.png" Width="16" Height="16" ToolTip="iRacing Connection Status"/>
        </StackPanel>
    </Grid>
</Window>
